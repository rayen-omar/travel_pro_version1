<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="seq_travel_reservation" model="ir.sequence">
        <field name="name">Réservation Voyage</field>
        <field name="code">travel.reservation</field>
        <field name="prefix">RES-%(year)s-</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
    </record>

    <record id="seq_travel_invoice_client" model="ir.sequence">
        <field name="name">Facture Client Travel</field>
        <field name="code">travel.invoice.client</field>
        <field name="prefix">FAC-</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
    </record>

    <record id="seq_travel_withholding" model="ir.sequence">
        <field name="name">Retenue Fournisseur</field>
        <field name="code">travel.withholding</field>
        <field name="prefix">RET-</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
    </record>

    <record id="seq_travel_purchase" model="ir.sequence">
        <field name="name">Achat Travel</field>
        <field name="code">travel.purchase</field>
        <field name="prefix">ACH-</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
    </record>

    <record id="seq_cash_register" model="ir.sequence">
        <field name="name">Caisse</field>
        <field name="code">cash.register</field>
        <field name="prefix">CAISSE-</field>
        <field name="padding">3</field>
        <field name="number_next">1</field>
    </record>

    <record id="seq_cash_register_operation" model="ir.sequence">
        <field name="name">Opération de Caisse</field>
        <field name="code">cash.register.operation</field>
        <field name="prefix">OP-%(year)s-%(month)s-</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
    </record>
</odoo>